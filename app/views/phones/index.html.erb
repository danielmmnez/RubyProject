<h1>Phones</h1>
<ul>
    <% @phones.each do |phone| %>
        <li>
            <div style="display:flex;justify-content:space-between;margin:0em 5em;">
                <strong>Name: </strong>
                <%= phone.name %>
                <strong>Brand: </strong>
                <%= phone.brand%>
                <strong>Price: </strong>
                <%= phone.price%>
                <a href="<%=phone_path(phone)  %>">See more</a>
            </div>
        </li>
    <% end %>
</ul>
<%= link_to "New Phone", new_phone_path %>